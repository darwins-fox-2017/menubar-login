<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>idex</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>


<body>
    <h2>USERS</h2>

    <table style="width:100%">
        <tr>
           <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>role</th>
            <th>action</th>
        </tr>
        <% dataUsers.forEach(function(user){ %>
        <tr>
            <td><%= user.id%></td>
            <td><%= user.username%></td>
            <td><%= user.email%></td>
            <td><%= user.role%></td>
            <td><a href="/users/delete/<%=user.id%>"> delete</a></td>
        </tr>
     <% }); %>
    </table>
    
    <% menu.forEach(function(menu){  %>
      <a href="<%=menu.link%>" class="LinkButton"><%=menu.name%></a>

    <% })%>

</body>

</html>
